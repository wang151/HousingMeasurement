    <dimen name="mjsdk_base_blank_20">20dp</dimen>
    <dimen name="mjsdk_base_blank_25">25dp</dimen>
    <dimen name="mjsdk_base_blank_4">4dp</dimen>
    <dimen name="mjsdk_base_blank_5">5dp</dimen>
    <dimen name="mjsdk_base_blank_8">8dp</dimen>
    <dimen name="mjsdk_common_font_big">16sp</dimen>
    <dimen name="mjsdk_common_font_biger">18sp</dimen>
    <dimen name="mjsdk_common_font_normal">14sp</dimen>
    <dimen name="mjsdk_common_font_small">12sp</dimen>
