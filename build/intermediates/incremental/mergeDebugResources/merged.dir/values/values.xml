    <dimen name="mjsdk_common_font_smaller2">10sp</dimen>
    <dimen name="mjsdk_common_font_super_big">30sp</dimen>
    <dimen name="mjsdk_common_font_title">20sp</dimen>
    <dimen name="mjsdk_common_horizontal_margin">@dimen/mjsdk_base_blank_16</dimen>
    <dimen name="mjsdk_common_item_height">50dp</dimen>
    <dimen name="mjsdk_common_line_size">0.8dp</dimen>
    <dimen name="mjsdk_common_line_wide">2dp</dimen>
    <dimen name="mjsdk_common_vertical_margin">@dimen/mjsdk_base_blank_5</dimen>
